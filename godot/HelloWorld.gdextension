[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1
reloadable = true

[libraries]
linux.debug.x86_64 = "res://../rust/target/debug/librust_extensions.so"
linux.release.x86_64 = "res://../rust/target/release/librust_extensions.so"
windows.debug.x86_64 = "res://../rust/target/debug/librust_extensions.dll"
windows.release.x86_64 = "res://../rust/target/release/librust_extensions.dll"
macos.debug = "res://../rust/target/debug/librust_extensions.dylib"
macos.release = "res://../rust/target/release/librust_extensions.dylib"
macos.debug.arm64 = "res://../rust/target/debug/librust_extensions.dylib"
macos.release.arm64 = "res://../rust/target/release/librust_extensions.dylib"
